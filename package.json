{
  "name": "loop-habit-tracker",
  "private": true,
  "description": "A simple static website created for Loop Habit Tracker",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "framer-motion": "^1.6.17",
    "gatsby": "^2.18.3",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.23",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-github-btn": "^1.1.1",
    "react-helmet": "^5.2.1",
    "react-intersection-observer": "^8.25.2",
    "react-responsive-carousel": "^3.1.51",
    "react-typography": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "loop",
    "habit tracker",
    "landing page"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FlashBlaze/loop-habit-tracker"
